{
  "name": "graphql-subscriptions",
  "version": "1.3.1",
  "description": "GraphQL subscriptions for node.js",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/apollostack/graphql-subscriptions.git"
  },
  "dependencies": {
    "iterall": "^1.3.0"
  },
  "peerDependencies": {
    "graphql": "^0.10.5 || ^0.11.3 || ^0.12.0 || ^0.13.0 || ^14.0.0 || ^15.0.0"
  },
  "scripts": {
    "prepare": "tsc",
    "clean": "rimraf dist coverage",
    "compile": "tsc",
    "pretest": "npm run compile",
    "test": "npm run testonly --",
    "posttest": "npm run lint",
    "lint": "tslint --project ./tsconfig.json ./src/**/*.ts",
    "watch": "tsc -w",
    "testonly": "mocha --reporter spec --full-trace ./dist/test/tests.js ./dist/test/asyncIteratorSubscription.js",
    "coverage": "nyc npm run test",
    "coverage_old": "node ./node_modules/nyc cover _mocha -- --full-trace ./dist/test/tests.js ./dist/test/asyncIteratorSubscription.js",
    "prepublishOnly": "npm run clean && npm run compile"
  },
  "devDependencies": {
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.3",
    "@types/sinon": "^9.0.5",
    "@types/sinon-chai": "^3.2.4",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "graphql": "^15.3.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.3",
    "sinon-chai": "^3.5.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "license": "MIT"
}
